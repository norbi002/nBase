  cow: '&aKruffka mleczna'
  chicken: '&aKurczaczek z KFC'
  villager: '&aWiesniak z wioski'
  bat: '&aBatMan'
  sheep: '&aOwca terrorystkka'
  pig: '&aProsie'
  iron golem: '&aIron Man'
  
  # [- Wrogie moby
  enderman: '&cSlenderman'
  zombie pigman: '&cCzlowiek swinia'
  creeper: '&cTykajaca bomba'
  ghast: '&cPlaczace dziecko'
