command /nBase:near:
	description: nBase » Lista graczy w zasiegu x kratek od gracza.
	executable by: players
	aliases: near
	trigger:
		if player has permission "nBase.near" or "nBase.*":
			set {_} to value "numbers.near" get of "plugins/Skript/scripts/nBase/config.yml"
			send "&9=================================================="
			send "  &7Zasieg: &6%{_}% &7kratek."
			send "  &7Gracze:"
			loop all players in radius {_} of player:
				add 1 to {_p}
				send "    &9» &6%loop-player% &8-- &a%distance between player and loop-player% &7kratek"
			if {_p} is not set:
				send "  &9» &6BRAK&7."
			send "&9=================================================="
		else:
			send "&7[&anBase&7]   &9» &cNie masz uprawnien! &8(nBase.near)"
