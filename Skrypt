	description: nBase » Strzelanie mobami.
	executable by: players
	aliases: cannon, shoot, shot
	trigger:
		if player has permission "nBase.cannon" or "nBase.*":
			if arg 1 is not set:
				send "&7[&anBase&7]   &9» &7Uzyj: &6/cannon <mob>"
			else:
				shoot arg-1 at speed 1.2
		else:
