	description: nBase » Ustawianie MOTD serwera.
	executable by: players
	aliases: setmotd
	trigger:
		if player has permission "nBase.setmotd" or "nBase.*":
			if arg 1 is not set:
				send "&9=================================================="
				send "  &7Uzyj: &6/setmotd <tekst>"
				send "  &7Motd: &7%server's motd%"
				send "&9=================================================="
			else:
				set motd to "%colored arg 1%"
				set {nBase::motd} to colored arg 1
				send "&7[&anBase&7]   &9» &7Zmieniono motd na &7%colored arg 1%&7."
		else:
			send "&7[&anBase&7]   &9» &cNie masz uprawnien! &8(nBase.setmotd)"
